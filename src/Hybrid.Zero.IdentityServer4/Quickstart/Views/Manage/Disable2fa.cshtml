@{
    ViewData["Title"] = L("Title");
}

<h2>@ViewData["Title"]</h2>

<div class="alert alert-warning" role="alert">
    <p>
        <span class="fa fa-exclamation-triangle"></span>
        <strong>@L("SubTitle")</strong>
    </p>
    <p>
        @L("Info") <a asp-action="ResetAuthenticatorWarning"></a>
    </p>
</div>

<div>
    <form asp-action="Disable2fa" method="post" class="form-group">
        <button class="btn btn-danger" type="submit">@L("Disable2FA")</button>
    </form>
</div>
